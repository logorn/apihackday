extends layout

block content
  .navbar.navbar-inverse.navbar-fixed-top
    .navbar-inner
      .container-fluid
        a.btn.btn-navbar(data-toggle='collapse', data-target='.nav-collapse')
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.brand(href='#')
          img(src='images/little_logo.png')
        .nav-collapse.collapse
          p.navbar-text.pull-right
            img(src='#{session.facebook[0].profile.picture.data.url}')

            a.navbar-link(href='#')  #{session.facebook[0].profile.name} 
  .container-fluid
    .row-fluid
      .span3
        .sidebar-nav
          ul.nav.nav-list
            li.nav-header Hackathon
            li(id='current_hackathon')
              img(src='images/hackathon.png')
            li.nav-header Developer
            li(id='ruby')
              a(href='#') Ruby
            li(id='python')
              a(href='#') Python
            li(id='javascript')
              a(href='#') JavaScript
            li(id='php')
              a(href='#', id='php') PHP
            li
              input.input-medium(type='text', placeholder='add another')
            
            li.nav-header Designer
            li(id='photoshop')
              a(href='#') Photoshop
            li(id='illustrator')
              a(href='#') Illustrator
            li(id='frontend')
              a(href='#') Frontend Design

